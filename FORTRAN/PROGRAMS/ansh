      WRITE(*,*)'ENTER VALUES OF A,B,C,D,X : '
      READ(*,*) A,B,C,D,X
      IF(X-D) 10,20,30
10    F=A*X*X+B*X+C
      WRITE(*,*)'VALUE OF FUNC. = ',F
      STOP
20    F=0
      WRITE(*,*)'VALUE OF FUNC. = ',F
      STOP
30    F=A*X*X+B*X-C
      WRITE(*,*)'VALUE OF FUNC. = ',F
      STOP
      END
